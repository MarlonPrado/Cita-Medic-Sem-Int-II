        function ubica_seccion(X,Y){
            y=Y;
            x=X;
            devolver_seccion=0;
            if (y>0 && y<10 && x>y && y<40-x){
                devolver_seccion = 1;
            }else if (x>30 && x<40 && y<x && 40-x<y){
                devolver_seccion = 2;
            }else if (y>30 && y<40 && x<y && x>40-y){
                devolver_seccion = 3;
            }else if (x>0 && x<10 && y>x && x<40-y){
                devolver_seccion = 4;
            }else if (x>10 && x<30 && y>10 && y<30){
                devolver_seccion = 5;
            }
            return devolver_seccion;
        }
